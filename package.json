{
	"name": "memdb",
	"version": "0.1.0",
	"description": "Lightweight in-memory DB/query utilities with live queries.",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"perf": "node ./performance/ramify.performance.ts",
		"postbuild": "tsc-alias -p tsconfig.json",
		"clean": "rimraf dist || rm -rf dist",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"prepublishOnly": "npm run build",
		"test": "npm run build && jest"
	},
	"peerDependencies": {
		"react": ">=18"
	},
	"devDependencies": {
		"@babel/core": "^7.28.5",
		"@babel/preset-env": "^7.28.5",
		"@babel/preset-typescript": "^7.28.5",
		"@cspell/eslint-plugin": "^9.3.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.10.1",
		"@types/react": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"babel-jest": "^30.2.0",
		"eslint": "^9.38.0",
		"jest": "^30.2.0",
		"jiti": "^2.6.1",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.46.2"
	},
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"in-memory",
		"database",
		"query",
		"react",
		"livequery",
		"observable"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/haroonwaves/ramify-db.git"
	},
	"author": "Usman Haroon",
	"license": "MIT"
}
